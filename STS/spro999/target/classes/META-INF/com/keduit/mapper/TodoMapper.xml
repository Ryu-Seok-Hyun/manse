<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.keduit.mapper.TodoMapper">


 <select id="getList" resultType="com.keduit.domain.TodoVO">
        SELECT tno, title, dueDate, writer, finished
        FROM t_todo
        ORDER BY tno DESC
    </select>


	 <select id="get" resultType="com.keduit.domain.TodoVO">
        SELECT * FROM t_todo WHERE tno = #{tno}
    </select>
    
    </mapper>